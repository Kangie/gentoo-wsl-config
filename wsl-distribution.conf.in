# See https://learn.microsoft.com/en-us/windows/wsl/build-custom-distro#add-the-wsl-distribution-configuration-file
# gentoo-wsl-config-@version@
[oobe]
command = @libexecdir@/wsl/oobe.sh
defaultUid = 1000
defaultName = "Gentoo"

[shortcut]
enabled = true
icon = @datadir@/wsl/gentoo.ico

[windowsterminal]
enabled = true
ProfileTemplate = @datadir@/wsl/terminal-profile.json
